      $(document).ready(function() {
         var elem = document.querySelector('.datepicker');
          var instance = M.Datepicker.init(elem,{
            disableWeekends: true,
            format: 'dd-mmmm-yyyy',
            i18n: {
              cancel: 'Cancelar',
              clear:	'Limpiar',
              done:	'Ok',
              weekdaysAbbrev: ['D','L','M','M','J','V','S'],
              months: [
                'Enero',
                'Febrero',
                'Marzo',
                'Abril',
                'Mayo',
                'Junio',
                'Julio',
                'Agosto',
                'Septiembre',
                'Octubre',
                'Noviembre',
                'Diciembre'
              ],
              monthsShort:
              [
                'Ene',
                'Feb',
                'Mar',
                'Abr',
                'May',
                'Jun',
                'Jul',
                'Ago',
                'Sep',
                'Oct',
                'Nov',
                'Dic'
              ],
                                
              weekdays:
              [
                'Domingo',
                'Lunes',
                'Martes',
                'Miercoles',
                'Jueves',
                'Viernes',
                'Sabado'
              ],
                                
              weekdaysShort:
              [
                'Dom',
                'Lun',
                'Mar',
                'Mie',
                'Jue',
                'Vie',
                'Sab'
              ],
                                
              weekdaysAbbrev:	['D','L','M','X','J','V','S']
            }
            

          });


          console.log('mi instancia ', instance);
        });  
    
  </script> 



